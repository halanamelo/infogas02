    imprima ("Digite a quilometragem");
    b:= leia();
    imprima (" Digite quantos litros de gasolina foram colocados no tanque");  
    c:= leia();
    d:= (b/a);
    e:= (km/l, ou, b/c);
    imprima ("Digite qual a distancia pecorrida ate o final do trecho");
    g:= leia();
Fim
